      cov_18rl6mkdqe().s[22]++;
      alert('更新に失敗しました');
    } finally {
      /* istanbul ignore next */
      cov_18rl6mkdqe().s[23]++;
      setIsSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_18rl6mkdqe().s[24]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    className: "bg-white rounded-lg shadow-md p-6",
    children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h3", {
      className: "text-xl font-bold mb-4",
      children: "投稿を編集"
    }), /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
      onSubmit: handleSubmit,
      className: "space-y-4",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("label", {
          htmlFor: "edit-title",
          className: "block text-sm font-medium text-gray-700 mb-1",
          children: "タイトル"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
          type: "text",
          id: "edit-title",
          value: title,
          onChange: e => {
            /* istanbul ignore next */
            cov_18rl6mkdqe().f[3]++;
            cov_18rl6mkdqe().s[25]++;
            return setTitle(e.target.value);
          },
          className: "w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500",
          maxLength: 100,
          disabled: isSubmitting
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("label", {
          htmlFor: "edit-author",
          className: "block text-sm font-medium text-gray-700 mb-1",
          children: "投稿者名"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
          type: "text",
          id: "edit-author",
          value: author,
          onChange: e => {
            /* istanbul ignore next */
            cov_18rl6mkdqe().f[4]++;
            cov_18rl6mkdqe().s[26]++;
            return setAuthor(e.target.value);
          },
          className: "w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500",
          maxLength: 50,
          disabled: isSubmitting
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("label", {
          htmlFor: "edit-content",
          className: "block text-sm font-medium text-gray-700 mb-1",
          children: "内容"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("textarea", {
          id: "edit-content",
          value: content,
          onChange: e => {
            /* istanbul ignore next */
            cov_18rl6mkdqe().f[5]++;
            cov_18rl6mkdqe().s[27]++;
            return setContent(e.target.value);
          },
          className: "w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500",
          rows: 5,
          maxLength: 140,
          disabled: isSubmitting
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex space-x-2",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          type: "submit",
          disabled: isSubmitting,
          className: "flex-1 bg-blue-500 text-white py-2 px-4 rounded-md hover:bg-blue-600 disabled:bg-gray-400 disabled:cursor-not-allowed transition-colors",
          children: isSubmitting ?
          /* istanbul ignore next */
          (cov_18rl6mkdqe().b[3][0]++, '保存中...') :
          /* istanbul ignore next */
          (cov_18rl6mkdqe().b[3][1]++, '保存')
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          type: "button",
          onClick: onCancel,
          disabled: isSubmitting,
          className: "flex-1 bg-gray-500 text-white py-2 px-4 rounded-md hover:bg-gray-600 disabled:bg-gray-400 disabled:cursor-not-allowed transition-colors",
          children: "キャンセル"
        })]
      })]
    })]
  });
}
